# Jobs Database Configuration
JOBS_POSTGRES_HOST=localhost
JOBS_POSTGRES_PORT=5432
JOBS_POSTGRES_DB=sn98_jobs
JOBS_POSTGRES_USER=sn98_user
JOBS_POSTGRES_PASSWORD=your_password_here

# Bittensor Network Configuration
NETUID=98
SUBTENSOR_NETWORK=finney

# Wallet path (default: ~/.bittensor/wallets)
BT_WALLET_PATH=/path/to/wallets

# Chain Configuration
CHAIN_ID=8453
RPC_URL=https://mainnet.base.org

# Rebalance Protocol Configuration
REBALANCE_CHECK_INTERVAL=100

# Executor Bot (for live mode on-chain execution)
EXECUTOR_BOT_URL=http://localhost:8000
EXECUTOR_BOT_API_KEY=your_api_key_here

# Miner Configuration
MINER_VERSION=2.0.0-rebalance
MODEL_INFO=rebalance-only
